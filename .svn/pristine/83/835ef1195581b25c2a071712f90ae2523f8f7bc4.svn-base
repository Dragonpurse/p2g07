package domein;

import java.util.List;

public class Stelling {
	
	private List<StellingAntwoord> antwoorden;
	private String stelling;
	
	public Stelling(String stelling, List<StellingAntwoord> antwoorden){
		setStelling(stelling);
		setAntwoorden(antwoorden);
	}

	public List<StellingAntwoord> getAntwoorden() {
		return antwoorden;
	}

	public void setAntwoorden(List<StellingAntwoord> antwoorden) {
		this.antwoorden = antwoorden;
	}

	public String getStelling() {
		return stelling;
	}

	public void setStelling(String stelling) {
		this.stelling = stelling;
	}

}
