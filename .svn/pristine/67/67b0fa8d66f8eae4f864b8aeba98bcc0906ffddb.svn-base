package domein;

import java.util.ArrayList;
import java.util.Calendar;

public class Document extends Onderdeel {

	private String titel;
	private String displayNaam;
	private String omschrijving;
	private ArrayList<String> kernwoorden;
	private String doelgroep;
	private String locatie;
	private Calendar beschikbaarVan;
	private Calendar beschikbaarTot;
	
	public Document(String titel, String omschrijving, ArrayList<String> kernwoorden, String doelgroep, String locatie){
		setTitel(titel);
		setOmschrijving(omschrijving);
		setKernwoorden(kernwoorden);
		setDoelgroep(doelgroep);
		setLocatie(locatie);	
	}

	public String getTitel() {
		return titel;
	}

	public void setTitel(String titel) {
		this.titel = titel;
	}

	public String getDisplayNaam() {
		return displayNaam;
	}

	public void setDisplayNaam(String displayNaam) {
		this.displayNaam = displayNaam;
	}

	public String getOmschrijving() {
		return omschrijving;
	}

	public void setOmschrijving(String omschrijving) {
		this.omschrijving = omschrijving;
	}

	public ArrayList<String> getKernwoorden() {
		return kernwoorden;
	}

	public void setKernwoorden(ArrayList<String> kernwoorden) {
		this.kernwoorden = kernwoorden;
	}

	public String getDoelgroep() {
		return doelgroep;
	}

	public void setDoelgroep(String doelgroep) {
		this.doelgroep = doelgroep;
	}

	public String getLocatie() {
		return locatie;
	}

	public void setLocatie(String locatie) {
		this.locatie = locatie;
	}

	public Calendar getBeschikbaarVan() {
		return beschikbaarVan;
	}

	public void setBeschikbaarVan(Calendar beschikbaarVan) {
		this.beschikbaarVan = beschikbaarVan;
	}

	public Calendar getBeschikbaarTot() {
		return beschikbaarTot;
	}

	public void setBeschikbaarTot(Calendar beschikbaarTot) {
		this.beschikbaarTot = beschikbaarTot;
	}
}
