package domein;

import java.util.ArrayList;

public class Document extends Onderdeel {

	private String locatie;
	
	public Document(String titel, String omschrijving, ArrayList<String> kernwoorden, String doelgroep, String locatie, String displayNaam, String displayOmschrijving){
		super(displayNaam, displayOmschrijving, titel, omschrijving, kernwoorden, doelgroep, "DOC");
		setLocatie(locatie);
	}

	public String getLocatie() {
		return locatie;
	}

	public void setLocatie(String locatie) {
		this.locatie = locatie;
	}
}
